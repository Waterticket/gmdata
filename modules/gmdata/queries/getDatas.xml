<query id="getDatas" action="select">
    <tables>
        <table name="gmdata_board" />
    </tables>
    <columns>
		<column name="game_token" alias="game_token" />
        <column name="name" alias="name" />
        <column name="datas" alias="datas" />
		<column name="mb_status" alias="mb_status" />
    </columns>
    <conditions>
		<condition operation="equal" column="game_token" var="game_token"/>
		<condition operation="equal" column="usr_srl" var="usr_srl" pipe="and" />
    </conditions>
</query>
